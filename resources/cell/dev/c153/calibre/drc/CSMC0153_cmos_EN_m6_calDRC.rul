     @A5.d: Min enclosure of A5 line end region beyond via4 region must be >=0.05um
     @(when via4 at 90 degree corner,one side of metal enclosure must be considered as line end region). 
     X = RECTANGLE ENC W5_drc A5_drc ABUT<90 OUTSIDE ALSO
     GOOD 0 0.05 OPPOSITE 0 0.05 OPPOSITE
     NOT W5_drc A5_drc
     Y = ENC [X] (A5_drc NOT INSIDE EXCL_N) < 0.05 OPPOSITE ABUT<90
     INT Y < 0.01 ABUT==90 INTERSECTING ONLY
}

A5.f {
     @A5.f: Min A5 area must be >=0.2um2
     AREA A5_drc <0.2 
}

//;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
//  5.10  Via5 (WT)
//;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
wt_nfuse=NOT WT_drc wtfuse_ring
WT.a {
     @WT.a Min&Max WT size must be == 0.238um
     L1001=NOT RECTANGLE wt_nfuse ==0.238 BY ==0.238 ORTHOGONAL ONLY
     NOT INSIDE L1001 EXCL_N
}

WT.b {
     @WT.b Min WT space must be >= 0.238um
     L1001=EXTERNAL WT_drc <0.238 REGION SINGULAR ABUT<90 
     NOT INSIDE L1001 EXCL_N
}

WT.c_d {
     @WT.c: Min enclosure of A5 region beyond WT must be >=0um
     @WT.d: Min enclosure of A5 line end beyond WT must be >=0.06um
     X = RECTANGLE ENC WT_drc A5_drc ABUT<90  OUTSIDE ALSO
     GOOD 0 0.06 OPPOSITE 0 0.06 OPPOSITE
     NOT WT_drc A5_drc
     Y = ENC [X] (A5_drc NOT INSIDE EXCL_N) < 0.06 OPPOSITE ABUT<90
     INT Y < 0.001 ABUT==90 INTERSECTING ONLY
}

