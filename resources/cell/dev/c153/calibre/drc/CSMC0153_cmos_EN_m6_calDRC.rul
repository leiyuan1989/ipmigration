w1_fuse=AND W1_drc FUSE_drc
w2_fuse=AND W2_drc FUSE_drc
w3_fuse=AND W3_drc FUSE_drc
w4_fuse=AND W4_drc FUSE_drc
w5_fuse=AND W5_drc FUSE_drc
