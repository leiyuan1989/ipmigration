      L1001=SIZE bpm5 BY 17.5 UNDEROVER
      NOT INSIDE L1001 EXCL
}

MCT.c2 {
      @MCT.c2: Max dimension of M4 used for capacitor bottom plate must be <=35um
      L1001=SIZE bpm4 BY 17.5 UNDEROVER
      NOT INSIDE L1001 EXCL
}

MCT.d {
      @MCT.d: Min spacing between two capacitor MCTs must be <=1.02um
      L1001=EXTERNAL mctcap <1.02 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.e {
      @MCT.e: Min spacing between capacitor MCT and dummy MCT ring must be <=0.8um
      L1001=EXTERNAL mctcap mctring <0.8 REGION SINGULAR ABUT<90 
      L1002=EXTERNAL mctring <0.8 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      NOT INSIDE L1002 EXCL
}

MCT.f1 {
      @MCT.f1: Min spacing between two M5 used for capacitor bottom plates must be <=0.68um
      L1001=EXTERNAL bpm5 <0.68 REGION SINGULAR ABUT<90 
      L1002=EXTERNAL bpm5 dumpm5<0.68 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      NOT INSIDE L1002 EXCL
}

MCT.f2 {
      @MCT.f2: Min spacing between two M4 used for capacitor bottom plates must be <=0.68um
      L1001=EXTERNAL bpm4 <0.68 REGION SINGULAR ABUT<90 
      L1002=EXTERNAL bpm4 dumpm4 <0.68 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      NOT INSIDE L1002 EXCL
}

MCT.g1 {
      @MCT.g1: Min spacing of MCT to MVn or MVn-1 must be <=0.4um
      L1001=EXTERNAL WT_drc MCT_drc <0.4 REGION SINGULAR ABUT<90  
      NOT INSIDE L1001 EXCL
      CUT WT_drc MCT_drc
}

MCT.g2 {
      @MCT.g2: Min spacing of MCT to MVn or MVn-1 must be <=0.4um
      L1001=EXTERNAL W5_drc MCT_drc <0.4 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      CUT W5_drc MCT_drc
}

captopwt=AND WT_drc MCT_drc
capbotwt=(WT_drc AND bpm5) NOT MCT_drc

captopw5=AND W5_drc MCT_drc
capbotw5=(W5_drc AND bpm4) NOT MCT_drc

MCT.h1 {
      @MCT.h1: Min spacing between two WT which are covered by MCT must be <=1.7um
      L1001=EXTERNAL captopwt <1.7 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.h2 {
      @MCT.h2: Min spacing between two W5 which are covered by MCT must be <=1.7um
      L1001=EXTERNAL captopw5 <1.7 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.i1 {
      @MCT.i1: Min spacing between two WT which are covered by M5 must be <=1.7um
      L1001=EXTERNAL capbotwt <1.7 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.i2 {
      @MCT.i2: Min spacing between two W5 which are covered by M4 must be <=1.7um
      L1001=EXTERNAL capbotw5 <1.7 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.j1 {
      @MCT.j1: Min extension of MCT beyond WT must be <=0.24um
      L1001=ENCLOSURE WT_drc MCT_drc <0.24 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.j2 {
      @MCT.j2: Min extension of MCT beyond W5 must be <=0.24um
      L1001=ENCLOSURE W5_drc MCT_drc <0.24 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.k1 {
      @MCT.k1: Min extension of M5 beyond MV5 or MV4 must be <=0.12um
      L1001=ENCLOSURE WT_drc bpm5 <0.12 REGION SINGULAR ABUT<90 
      L1002=ENCLOSURE W5_drc bpm5 <0.12 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      NOT INSIDE L1002 EXCL
}

MCT.k2 {
      @MCT.k2: Min extension of M4 beyond MV4 or MV3 must be <=0.12um
      L1001=ENCLOSURE W5_drc bpm4 <0.12 REGION SINGULAR ABUT<90 
      L1002=ENCLOSURE W4_drc bpm4 <0.12 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
      NOT INSIDE L1002 EXCL
}

MCT.l1 {
      @MCT.l1: Min extension of M5 beyond MCT must be <=0.34um
      L1001=ENCLOSURE MCT_drc A5_drc <0.34 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.l2 {
      @MCT.l2: Min extension of M4 beyond MCT must be <=0.34um
      L1001=ENCLOSURE MCT_drc A4_drc <0.34 REGION SINGULAR ABUT<90 
      NOT INSIDE L1001 EXCL
}

MCT.m1 {
      @MCT.m1: Min extension of MCTM5 beyond M5 must be >= 2.0um      
