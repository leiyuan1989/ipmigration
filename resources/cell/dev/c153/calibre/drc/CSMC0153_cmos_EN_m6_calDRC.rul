#IFDEF CHECK_SlotRule_C
SLOT.MTT.c {
      @SLOT.MTT.c: Minimum width of a metal5 line which is connected to wide metal5 line must be >=10um.
      L34096= COINCIDENT OUTSIDE EDGE (ttax NOT INSIDE EXCL) (ttgu NOT INSIDE EXCL)
