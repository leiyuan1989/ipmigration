w2fuse_ring=INSIDE W2_drc a1fuse_ring
w3fuse_ring=INSIDE W3_drc a2fuse_ring
w4fuse_ring=INSIDE W4_drc a3fuse_ring
w5fuse_ring=INSIDE W5_drc a4fuse_ring
wtfuse_ring=INSIDE WT_drc a5fuse_ring

