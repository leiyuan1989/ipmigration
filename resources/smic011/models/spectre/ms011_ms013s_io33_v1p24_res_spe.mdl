//*Spectre Model Format
simulator lang=spectre  insensitive=yes

// *
// * no part of this file can be released without the consent of smic.
// ************************************************************************************************
// *    smic 0.11um mixed signal 1p8m(1p7m, 1p6m, 1p5m) 1.2v/3.3v spice model (for Spectre only)  *
// ************************************************************************************************
// *
// * release version    : 1.24
// *
// * release date       : 1/16/2015
// *
// * simulation tool    : Cadence spectre V10.1.1
// *
// *   resistor         :
// *        *----------------------------------------------------*
// *        |       resistor type                   | 1.2v/3.3v  |
// *        |====================================================|
// *        | silicide n+ diffusion                 |   rndif    |
// *        |----------------------------------------------------|
// *        | silicide p+ diffusion                 |   rpdif    |
// *        |----------------------------------------------------|
// *        | silicide n+ poly                      |    rnpo    |
// *        |----------------------------------------------------|
// *        | silicide n+ poly (three terminal)     |   rnpo_3t  |
// *        |----------------------------------------------------|
// *        | silicide p+ poly                      |    rppo    |
// *        |----------------------------------------------------|
// *        | silicide p+ poly (three terminal)     |   rppo_3t  |
// *        |----------------------------------------------------|
// *        | nwell under sti                       |   rnwsti   |
// *        |----------------------------------------------------|
// *        | nwell under aa                        |   rnwaa    |
// *        |----------------------------------------------------|
// *        | non-silicide n+ diffusion             |  rndifsab  |
// *        |----------------------------------------------------|
// *        | non-silicide p+ diffusion             |  rpdifsab  |
// *        |----------------------------------------------------|
// *        | non-silicide n+ poly                  |  rnposab   |
// *        |----------------------------------------------------|
// *        | non-silicide n+ poly (three terminal) | rnposab_3t |
// *        |----------------------------------------------------|
// *        | non-silicide p+ poly                  |  rpposab   |
// *        |----------------------------------------------------|
// *        | non-silicide p+ poly (three terminal) | rpposab_3t |
// *        |----------------------------------------------------|
// *        | High Resistance Poly                  |   rhrpo    |
// *        |----------------------------------------------------|
// *        | High Resistance Poly (three terminal) |  rhrpo_3t  |
// *        |----------------------------------------------------|
// *        |          metal 1                      |    rm1     |
// *        |----------------------------------------------------|
// *        |          metal 2                      |    rm2     |
// *        |----------------------------------------------------|
// *        |          metal 3                      |    rm3     |
// *        |----------------------------------------------------|
// *        |          metal 4                      |    rm4     |
// *        |----------------------------------------------------|
// *        |          metal 5                      |    rm5     |
// *        |----------------------------------------------------|
// *        |          metal 6                      |    rm6     |
// *        |----------------------------------------------------|
// *        |          metal 7                      |    rm7     |
// *        |----------------------------------------------------|
// *        |          metal 8                      |    rm8     |
// *        |----------------------------------------------------|
// *        |          ALPA                         |   ralpa    |
// *        *----------------------------------------------------*
// *
// *   the valid temperature range is from -40c to 125c
// *
// ******************************************************************
// *                         resistor model                         *
// ******************************************************************
// ******************************************************************
// *                       silicide resistors                       *
// ******************************************************************
// ******************************************************************
// *                   silicide n+ diffusion resistance             *
// ******************************************************************
model rndif resistor
+ coeffs = [3.43e-05 4.07e-04]
+tc1 = 0.00323  tc2 = 2.784e-07 etch = -2.67e-08+ddw_rndif 
+tnom  = 25 rsh = 7.15+drsh_rndif 
// ******************************************************************
// *                   silicide p+ diffusion resistance             *
// ******************************************************************
model rpdif resistor
+ coeffs = [4.22e-05 3.14e-04]
+tc1 = 0.00309  tc2 = 3.6e-07  etch = -6.62e-09+ddw_rpdif 
+tnom  = 25 rsh = 8+drsh_rpdif 
// ******************************************************************
// *                  silicide n+ poly resistance                   *
// ******************************************************************
model rnpo resistor
+ coeffs = [1.59e-03 2.38e-03]
+tc1 = 0.0031  tc2 = 1.48e-07  etch = -3.05e-08-2.7E-09+ddw_rnpo 
+tnom  = 25 rsh = 7.5+drsh_rnpo
// ******************************************************************
// *          silicide n+ poly resistance (three terminal)          *
// ******************************************************************
model rnpo_3t resistor
+ coeffs = [1.59e-03 2.38e-03]
+tc1 = 0.0031  tc2 = 1.48e-07  etch = -3.05e-08-2.7E-09+ddw_rnpo_3t 
+tnom  = 25 rsh = 7.5+drsh_rnpo_3t
+cj =9.31e-05+dcj_rnpo_3t  cjsw=8.75e-11+dcjsw_rnpo_3t 
// ******************************************************************
// *                  silicide p+ poly resistance                   *
// ******************************************************************
model rppo resistor
+ coeffs = [9.28e-04 1.94e-03]
+tc1 = 0.00299  tc2 = 3.24e-07  etch = -1.66e-08-2.7E-09+ddw_rppo 
+tnom  = 25 rsh = 7.85+drsh_rppo
// ******************************************************************
// *          silicide p+ poly resistance (three terminal)          *
// ******************************************************************
model rppo_3t resistor
+ coeffs = [9.28e-04 1.94e-03]
+tc1 = 0.00299  tc2 = 3.24e-07  etch = -1.66e-08-2.7E-09+ddw_rppo_3t 
+tnom  = 25 rsh = 7.85+drsh_rppo_3t
+cj =9.31e-05+dcj_rppo_3t  cjsw=8.75e-11+dcjsw_rppo_3t 
// *
// ******************************************************************
// *                     non-silicide resistors                     *
// ******************************************************************
// ******************************************************************
// *                   nwell resistance under sti                   *
// ******************************************************************
model rnwsti resistor
+ coeffs = [1.71e-02 5.14e-05]
+tc1 = 0.00273  tc2 = 1.43e-05 etch = 2.52e-07+ddw_rnwsti 
+tnom  = 25 rsh = 1080+drsh_rnwsti 
// ******************************************************************
// *                    nwell resistance under aa                   *
// ******************************************************************
model rnwaa resistor
+ coeffs = [1.37e-02 1.96e-04]
+tc1 = 0.00334  tc2 = 1.44e-05  etch = 1.26e-07+ddw_rnwaa 
+tnom  = 25 rsh = 446+drsh_rnwaa 
// ******************************************************************
// *                 non-silicide n+ diffusion resistance           *
// ******************************************************************
model rndifsab resistor
+ coeffs = [1.98E-04 7.50E-04]
+tc1 = 1.35E-03 tc2 = 8.02E-07 etch = 1.00E-08+ddw_rndifsab etchl = -2.64e-7
+tnom  = 25 rsh = 69.20+drsh_rndifsab 
// ******************************************************************
// *                 non-silicide p+ diffusion resistance           *
// ******************************************************************
model rpdifsab resistor
+ coeffs = [-4.23E-05 2.74E-04]
+tc1 = 1.34E-03 tc2 = 1.00E-06 etch = 1.00E-08+ddw_rpdifsab etchl = -2.74e-7
+tnom  = 25 rsh = 139.50+drsh_rpdifsab 
// ******************************************************************
// *                 non-silicide n+ poly resistance                *
// ******************************************************************
model rnposab resistor
+ coeffs = [-5.53E-05 -5.75E-04]
+tc1 = -9.93E-04 tc2 = 1.07E-06 etch = 1.68E-08-2.7E-09+ddw_rnposab etchl = -1.33e-7
+tnom  = 25 rsh = 275.50+drsh_rnposab 
// ******************************************************************
// *                 non-silicide p+ poly resistance                *
// ******************************************************************
model rpposab resistor
+ coeffs = [-5.07E-05 -7.96E-05]
+tc1 = -5.75E-05 tc2 = 6.10E-07 etch = 1.28E-08-2.7E-09+ddw_rpposab etchl = -2.68e-7
+tnom  = 25 rsh = 321.5+drsh_rpposab 
// ******************************************************************
// *         non-silicide n+ poly resistance (three terminal)       *
// ******************************************************************
model rnposab_3t resistor
+ coeffs = [-5.53E-05 -5.75E-04]
+tc1 = -9.93E-04 tc2 = 1.07E-06 etch = 1.68E-08-2.7E-09+ddw_rnposab_3t etchl = -1.33e-7
+tnom  = 25 rsh = 275.50+drsh_rnposab_3t 
+cj =9.31e-05+dcj_rnposab_3t  cjsw=8.75e-11+dcjsw_rnposab_3t 
// ******************************************************************
// *         non-silicide p+ poly resistance (three terminal)       *
// ******************************************************************
model rpposab_3t resistor
+ coeffs = [-5.07E-05 -7.96E-05]
+tc1 = -5.75E-05 tc2 = 6.10E-07 etch = 1.28E-08-2.7E-09+ddw_rpposab_3t etchl = -2.68e-7
+tnom  = 25 rsh = 321.5+drsh_rpposab_3t 
+cj =9.31e-05+dcj_rpposab_3t  cjsw=8.75e-11+dcjsw_rpposab_3t 
// ******************************************************************
// *                non-silicide hr poly resistance                 *
// ******************************************************************
model rhrpo resistor
+coeffs =[-2.34e-05 -1.20e-04] 
+tc1 = -6.77e-04 tc2 = 2.08e-06 etch = 2.27e-08-2.7E-09+ddw_rhrpo 
+tnom  = 25.0 rsh = 963+drsh_rhrpo 
// ******************************************************************
// *        non-silicide hr poly resistance (three terminal)        *
// ******************************************************************
model rhrpo_3t resistor
+coeffs =[-2.34e-05 -1.20e-04] 
+tc1 = -6.77e-04 tc2 = 2.08e-06 etch = 2.27e-08-2.7E-09+ddw_rhrpo_3t 
+tnom  = 25.0 rsh = 963+drsh_rhrpo_3t 
+cj = 9.31e-05+dcj_rhrpo_3t  cjsw = 8.75e-11+dcjsw_rhrpo_3t

// *******************************************************************
// *                         metal resistors                         *
// *******************************************************************
// ******************************************************************
// *                      metal 1 resistance                        *
// ******************************************************************
model rm1 resistor

+tc1 = 3.62e-03  tc2 = -6.98e-07  etch = -1.02E-08+ddw_rm1 
+tnom  = 25 rsh = 0.11+drsh_rm1 
+cj =4.08E-05+dcj_rm1  cjsw =1.17E-10+dcjsw_rm1
