+tnom  = 25 rsh = 0.065+drsh_rm6 
+cj =7.72E-06+dcj_rm6  cjsw =1.15E-10+dcjsw_rm6
// ******************************************************************
// *                      metal 7 resistance                        *
// ******************************************************************
model rm7 resistor

+tc1 = 3.70e-03  tc2 = 7.77e-07  etch = -1.06E-08+ddw_rm7 
+tnom  = 25 rsh = 0.065+drsh_rm7 
+cj =6.61E-06+dcj_rm7  cjsw =1.17E-10+dcjsw_rm7
