******************************************************************
*                      Metal 6 resistance                        * 
******************************************************************
.model rm6_rf r
*metal resistor
+TC1R = 3.78E-03      TC2R = -3.83E-07               DW = '-5.6494E-09+DDW_RM6_RF'
+TREF = 25            RSH  = '0.0145+DRSH_RM6_RF'
*
